                  <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
               </columnResizePolicy></TableView>
            <Button fx:id="btnModificarPaciente" alignment="CENTER" layoutX="1078.0" layoutY="197.0" mnemonicParsing="false" onAction="#clickModificarPaciente" prefHeight="40.0" prefWidth="171.0" text="Modificar" textFill="WHITE" />
            <Button fx:id="btnBorrarPaciente" alignment="CENTER" layoutX="1078.0" layoutY="263.0" mnemonicParsing="false" onAction="#borrarPaciente" prefHeight="40.0" prefWidth="171.0" text="Borrar" textFill="WHITE" />
            <Button fx:id="btnRefrescar" layoutX="971.0" layoutY="590.0" mnemonicParsing="false" onAction="#clickRefrescar" text="Refrescar">
               <font>
                  <Font size="14.0" />
               </font></Button>
			</children>
		</AnchorPane>
	</center>
